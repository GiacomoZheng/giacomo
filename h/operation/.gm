: ; operation
(
	"a set action on itself",
	⊆ set.action,
	(
		∀p: ∈ operation,
		p.D = p.S,

		; attributes
			p.closure:
			(
				"an nonempty closure on this operation",
				⊆ set.nonempty_sub(p.D),
				∀S: ∈ p.closure,
				∀{x, y}: ⊆ S, p(x)(y) ∈ S
			),

			p.reverse(∀x: ∈ p.D)(∀y: ∈ p.D): = p(y)(x),
			p.reverse.D: = p.D,
	)
)