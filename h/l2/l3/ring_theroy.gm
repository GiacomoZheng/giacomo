s"irreducible polynomials in P[R](x) are linears and irreducible quadratic": ; !
(
	@gm.h.Ring,
	∀x: ∈ ◉, ; !
	p(x): ∈ P[h.R](x),
	S: = {n|∀n: ∈ h.Z, n ≥ 0, n ≤ deg(p(x))},
	∃α[∀n: ∈ S]: ∈ s.C,
	∀y: ∈ h.R, p(y) = ×[∀n: ∈ S](y - α[n]),
	p(y) ∈ h.R ⇒ p(y) = conj(p(y))
	⇒ p(y) = conj(×[∀n: ∈ S](y - α[n]))
	⇒ p(y) = ×[∀n: ∈ S](y - conj(α[n])),
	q(x): = ×[∀n: ∈ S](x - conj(α[n])),
	∀(
		!(|R| ∈ h.N),
		(∀y: ∈ h.R, q(y) = p(y))
	)
	⇒ p = q,
	P[h.R](x) ∈ unique_factorization_ring,
	∃S1: ⊆ S, α[∀n: ∈ S1] ∈ h.R,
	S2: = S \ S1,
	∀n: ∈ S2, ∃β[n]: ∈ s.C, conj(β[n]) ∈ {β[m]|∀m: ∈ S2},
	p(y) = ×[∀n: ∈ S1](y - α[n]) × ×[∀n: ∈ S2](y - β[n])
	= ×[∀n: ∈ S1](y - α[n]) × ×[{j1, j2}: ⊆ S2](y^2 - (β[j1]+β[j2]) * y + β[j1] * β[j2]) ; !
	■
),

s"rational root test":
(
	@gm.h.Ring,
	∀x: ∈ ◉, ; !
	∀f(x): ∈ P[h.Z](x),
	n: = deg(f(x)),
	a: = f(x).a,
	∀r: ∈ root[h.Q](f), ; *
	∃(
		r = 0,
		(
			∃{p, q}: ⊆ h.Z \ {0}, r = p / q,
			a[0] / p ∈ h.Z,
			a[n] / q ∈ h.Z
		)
	)■
),

s"Eisenstein's criterion": ; + add it to cheeting paper,
(
	@gm.h.Ring,
	∀x: ∈ ◉, ; !
	∀f(x): ∈ P[h.Z](x),
	n: = deg(f(x)),
	S: = {m|∀m: ∈ h.Z, 0 ≤ m ≤ n},
	a: = f(x).a,
	(
		∃p: ∈ prime_number,
		!(a[0] / p^2),
		!(a[n] / p),
		a[∀n: ∈ S\{n}]/ p
	)
	⇒ f(x) ∈ irreducible(R)■
)

