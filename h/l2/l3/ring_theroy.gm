s"enough intersection points make two polynomial the same":
(
	@gm.h.Ring,
	∀P[R](x): ∈ polynomial_ring,
	∀{f(x), g(x)}: ⊆ P[R](x),
	∃m: ∈ h.Z, m > deg(f(x)), m > deg(g(x)),
	(∃A: ⊆ R, |A| = m, (∀a: ∈ A, f(a) = g(a))) ⇒ f = g
	■
),

s"intersection at the infinite domain make two polynomial the same":
(
	@gm.h.Ring,
	∀P[R](x): ∈ polynomial_ring,
	∀{f(x), g(x)}: ⊆ P[R](x),
	∀(
		!(|R| ∈ h.N),
		(∀a: ∈ R, f(a) = g(a))
	) ⇒ f = g
	■
),

