		∃g'': ∈ G, g' * g'' = e,
		g' * g = e ⇐
		(
			g' * g = g' * (g * e) = g' * (g * (g' * g'')) = (g' * (g * g')) * g'' = (g' * e) * g'' = g' * g'' = e
		),
		e * g = g ⇐
		(
			e * g = (g * g') * g = g * (g' * g) = g * e ; due to the proof above, the left inverse is also the right one
		)
