: ; vector_space
(
	⊆ abelian_group, ; "+" defined there
	(
		; +
		∀V: ∈ vector_space,
		V.×: ∈ symbol, ; !
		V.F: ∈ field,

		∀v: ∈ V,
		∀a: ∈ V.F, a V.× v = v V.× a ∈ V, ; closure, with the closure in abelian group
		V.F.1 V.× v = v, ; identity
		∀b: ∈ V.F, (a V.F.× b) V.× v = a V.× (b V.× v), ; compatibility

		∀u: ∈ V, a V.× (u V.+ v) = (a V.× u) V.+ (a V.× v), ; distribution
		(a V.+ b) V.× v = (a V.× v) V.+ (c V.× v)
	), ; to deal with the temporaries
	[ property: ... ]
		; about alias
		; end

		; + about inheritance
			sub(∀V: ∈ vector_space):
			(
				⊆ vector_space,
				⊆ group.sub(V),
				∀S: ∈ sub(V),
				&S.+& = &V.+&,
				S.0 = V.0,
				&S.×& = &V.×&,
				S.F = V.F
			),

			min_sub[∀G: ∈ vector_space](∀S: ⊆ G): = ∩[∀H: ∈ sub(G), S ⊆ H](H), ; !

			ker(∀f: ∈ homomorphism): = group.ker(f)

			neg[∀V: ∈ vector_space]: = group.inv[V],
		; end

		independent(∀V: ∈ vector_space):
		(
			"independent subset of this vector space",
			⊆ set.sub(V),
			∀I: ∈ independent(V),
			∀c[∀α: ∈ I]: ∈ V.F,
			V.+[∀α: ∈ I](c[α] V.× α) = V.0 ⇒ (∀α: ∈ I, c[α] = V.F.0) ; ? as an abelian group, order of addition is unimportant
		),

		combination[∀V: ∈ vector_space](∀S: ∈ V):
		(
			⊆ ◉,
			∀x: ∈ combination[V](S),
			∃c[∀α: ∈ S]: ∈ V.F,
			x = V.+[∀α: ∈ I](c[α] V.× α) ; means it is the elements of V
		),

		span[∀V: ∈ vector_space](S): = {∀x: ∈ combination[V](S)},

),