: ; integral domain
(
	⊆ domain, ; !
	⊆ commutative_ring,
	; location
		; about inheritance
			divisor[∀R: ∈ integral_domain]: = commutative_ring.divisor[R],

			multiple[∀R: ∈ integral_domain]: = commutative_ring.multiple[R],

			associated[∀R: ∈ integral_domain](∀a: ∈ R):
			(
				⊆ divisor[R](a),
				⊆ multiple[R](a)
				[ equivalently```
					∀b: ∈ associated[R](a), ∃u: ∈ unit(R), b = u R.× a
				``` ]
			),

			irreducible(∀R: ∈ integral_domain):
			(
				"p = ab then a is a unit or b is a unit",
				⊆ ◉,
				∀ir: ∈ irreducible(R),
				ir ∈ R,
				!(ir ∈ unit(R)), ; ?
				×: = &R.×&
				∀(d1, d2): ∈ R, d1 × d2 = ir ⇒ ∃(d1 ∈ unit(R), d2 ∈ unit(R))
			),

			prime(∀R: ∈ integral_domain): = commutative_rng.prime(R)
		; end
)