: ; commutative_ring
(
	⊆ ring,
	⊆ rng.commutative_rng ; ?

	; 
		; about alias
			ED: = Euclidean_domain,
		; end
		
		; about generalizing
			unit(∀R: ∈ commutative_ring): = ring.unit(R),
		; end
		
		irreducible(∀R: ∈ ring):
		(
			⊆ ◉,
			∀ir: ∈ irreducible(R),
			ir ∈ R,
			!(ir = R.0),
			!(ir ∈ unit(R)),
			×: = &R.×&
			∀(d1, d2): ∈ R, d1 × d2 = ir ⇒ ∃(d1 ∈ unit(R), d2 ∈ unit(R))
		),

		prime(∀R: ∈ rng): ; the prime elements
		(
			⊆ ◉,
			∀p: ∈ prime(R),
			p ∈ R,
			!(p = R.0),
			!(p ∈ unit(R)),
			@R,
				∀{a, b}: ∈ R,
				(∃c: ∈ R, c × p = a × b)
				⇒ ∀(
					(∃d: ∈ R, d × p = a),
					(∃e: ∈ R, e × p = b)
				)
			■
		)
),