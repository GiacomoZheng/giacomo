: ; module, regular module here, or named as left module
(
	"rng module",
	⊆ abelian_group,
	(
		"the π here means 'πολλαπλασιασμός' meaning multiplication in Greek",
		∀M: ∈ module,
		M.π: ∈ action, ; here it is the rng (left) action
		M ⊆ M.π.S,
		M.R: = M.π.D
	),
	; location
		; about inheritace
			neg[∀V: ∈ module]: = abelian_group.neg[V],
		; end
),
