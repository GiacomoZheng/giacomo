: ; commutative_ring
(
	⊆ ring,
	⊆ commutative_rng,
	;
		; about alias
		; end

		; about inheritance
			unit(∀R: ∈ commutative_ring): = ring.unit(R),

			ideal(∀R: ∈ commutative_ring): = commutative_rng.ideal(R),

			principle_ideal(∀R: ∈ commutative_ring): = commutative_rng.principle_ideal(R),

			proper_ideal(∀R: ∈ commutative_ring): = commutative_rng.proper_ideal(R),

			common_divisor[∀R: ∈ commutative_ring]: = commutative_rng.common_divisor[R],

			gcd[∀R: ∈ commutative_ring]: = commutative_rng.gcd[R],

			divisor[∀R: ∈ commutative_ring]: = commutative_rng.divisor[R],

			multiple[∀R: ∈ commutative_ring]: = commutative_rng.multiple[R],
		; end

		associated[∀R: ∈ commutative_ring](∀a: ∈ R):
		(
			⊆ divisor[R](a),
			⊆ multiple[R](a)
			[ equivalently```
				∀b: ∈ associated[R](a), ∃u: ∈ unit(R), b = u R.× a
				```
				? all the zero divior are only associated itself
			 ]
		),

		irreducible(∀R: ∈ commutative_rng):
		(
			"p = ab then a is a unit or b is a unit",
			⊆ set.element(R),
			∀ir: ∈ irreducible(R),
			!(ir ∈ unit(R)), ; ?
			@R,
			∀(d1, d2): ∈ R, d1 × d2 = ir ⇒ ∃(d1 ∈ unit(R), d2 ∈ unit(R))
			■
		),

		P[∀R: ∈ commutative_ring](∀x: ∈ ◉):
		(
			"a polynomial ring over R",
			S[∀n: ∈ h.N]: = {m|h.N.0 ≤ m < n}[∀m: ∈ h.N], ; pre definition
			= {+[∀n: ∈ S[m]](c[n] R.× (x R.^ n))}[∀m: ∈ h.N, ∀n: ∈ S[m], ∀c[∀n: ∈ S[m]]: ∈ R]
			; * if the x don't in any super ring of the R, the error would occurs
		),
),
