: ; vector_space
(
	"field module",
	⊆ ring.module, ; "+" defined there
	(
		∀V: ∈ vector_space,
		V.F: = V.R,
		V.F ∈ field,
		V.μ ∈ field.action ; here it is the field (left) action
	),
	[ property: ... ]
		; about alias
		; end

		; + about inheritance
			sub(∀V: ∈ vector_space):
			(
				⊆ vector_space,
				⊆ ring.module.sub(V), ; +
				∀S: ∈ sub(V),
				V.F = S.F,
				&V.μ& = &S.μ&
			),

			min_sub[∀G: ∈ vector_space](∀S: ⊆ G): = ∩[∀H: ∈ sub(G), S ⊆ H](H), ; !

			ker(∀f: ∈ homomorphism): = group.ker(f)

			neg[∀V: ∈ vector_space]: = group.inv[V],
		; end

		independent(∀V: ∈ vector_space):
		(
			"independent subset of this vector space",
			⊆ set.sub(V),
			∀I: ∈ independent(V),
			∀c[∀α: ∈ I]: ∈ V.F,
			V.+[∀α: ∈ I](c[α] V.× α) = V.0 ⇒ (∀α: ∈ I, c[α] = V.F.0) ; ? as an abelian group, order of addition is unimportant
		),

		combination[∀V: ∈ vector_space](∀S: ∈ V):
		(
			⊆ ◉, ; ?
			∀x: ∈ combination[V](S),
			∃c[∀α: ∈ S]: ∈ V.F,
			x = V.+[∀α: ∈ I](c[α] V.× α) ; means it is the elements of V
		),

		span[∀V: ∈ vector_space](∀S: ⊆ set.sub(V)): = {∀x: ∈ combination[V](S)},

		eigen(∀f: ∈ endomorphism): ; +
		(
			"eigen value of "
		)
),