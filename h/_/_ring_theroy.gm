s"enough intersection points make two polynomial the same":
(
	@gm.Ring,
	∀P[R](x): ∈ polynomial_ring,
	∀{f(x), g(x)}: ⊆ P[R](x),
	∃m: ∈ ℤ, m > deg(f(x)), m > deg(g(x)),
	(∃A: ⊆ R, |A| = m, (∀a: ∈ A, f(a) = g(a))) ⇒ f = g
	■
),

s"intersection over the infinite ring make two polynomials the same":
(
	@gm.Ring,
	∀P[R](x): ∈ polynomial_ring,
	∀{f(x), g(x)}: ⊆ P[R](x),
	∀(
		!(|R| ∈ ℕ),
		(∀a: ∈ R, f(a) = g(a))
	) ⇒ f = g
	■
),

