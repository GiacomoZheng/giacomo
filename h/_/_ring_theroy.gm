s"rational root test":
(
	@gm.Ring,
	∀x: ∈ ◉, ; !
	∀f(x): ∈ P[ℤ](x),
	n: = deg(f(x)),
	a: = f(x).a,
	∀r: ∈ root[ℚ](f), ; *
	∃(
		r = 0,
		(
			∃{p, q}: ⊆ ℤ \ {0}, r = p / q,
			a[0] / p ∈ ℤ,
			a[n] / q ∈ ℤ
		)
	)■
),

s"Eisenstein's criterion": ; + add it to cheeting paper,
(
	@gm.Ring,
	∀x: ∈ ◉, ; !
	∀f(x): ∈ P[ℤ](x),
	n: = deg(f(x)),
	S: = {m|∀m: ∈ ℤ, 0 ≤ m ≤ n},
	a: = f(x).a,
	(
		∃p: ∈ prime_number,
		!(a[0] / p^2),
		!(a[n] / p),
		a[∀n: ∈ S\{n}]/ p
	)
	⇒ f(x) ∈ irreducible(R)■
)

