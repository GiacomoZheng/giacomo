; + about Sylow's theorems
	s"the groups with size prime are cyclic"

	s"lemma: the center of p-group": ; +
	(
		∀G: ∈ group, ∀p: ∈ ℕ.prime, ∀n: ∈ ℕ,
		|G| = p^n ⇒ ¬(center(G) = {G.e})
	),

	s"Sylow's First Theorem": ; ! ex
	(
		; + after the writting of natural numbers
		∀G: ∈ finite_group,
		@ℕ,
		∀p: ∈ prime(ℕ), ; ?
			∀n: ∈ ℕ, m: = |G|/(p^n),
			∀(m ∈ ℕ, gcd(m, p) = 1) ⇒
			(
				∃H: ∈ sub(G), |H| = p^n
			)
		■
	),
; end
