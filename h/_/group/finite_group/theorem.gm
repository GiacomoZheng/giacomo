; about Sylow's theorems
	s"lemma: the center of p-group": ; +
	(
		∀G: ∈ group, ∀p: ∈ prime_number, ∀n: ∈ h.N,
		|G| = p^n ⇒ !(center(G) = {G.e})
	),

	s"Sylow's First Theorem": ; ! ex
	(
		; + after the writting of natural numbers
		∀G: ∈ finite_group,
		∀p: ∈ commutative_rng.prime(h.N), ; ?
		∀n: ∈ h.N, m: = |G|/(p^n),
		∀(m ∈ h.N, commutative_rng.gcd(m, p) = 1) ⇒
		(
			∃H: ∈ sub(G), |H| = p^n
		)
	),
; end
