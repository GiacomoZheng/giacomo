s"Lagrange Theorem":
(
	@giacomo.h.abstract_algebra,
	G: ∈ group, H: %subgroup% G,
	|G| ∈ N ⇒ |G|/|H| ∈ Z ■
),
; Proof: start
	; Lemma: start
		@giacomo.h.abstract_algebra,
		G: ∈ group, H: %subgroup% G, *: = &G.*&
		∀x: ∈ G, |H| = |x * H| ■
	; Lemma: end
	; Proof: start
		@giacomo.h.abstract_algebra,
		G: ∈ group, H: %subgroup% G, *: = &G.*&
		∀x: ∈ G,
		∃φ[x]:
		(
			∈ mapping,
			D: = H, C: = x × H,
			φ[x](∀h: ∈ H): = x * h
		),
		(
			(
				∀h: ∈ H, ∀h': ∈ H,
				φ[x](h) = φ[x](h') ⇒ x * h = x * h' ⇒ h = h'
			)
			⇒ φ[x] ∈ injection,
			(
				∀y: ∈ φ[x].C,
				∃h: ∈ H, y = x * h, φ[x](h) = y
			)
			⇒ φ[x] ∈ surjection
		)
		⇒ φ[x] ∈ bijection
		⇒ |φ[x].C| = |φ[x]|
		⇒ |H| = |x * H| ■
	; Proof: end
	@giacomo.h.abstract_algebra,
	G: ∈ group, H: %subgroup% G, *: = &G.*&
	(∀x: ∈ G, G.e ∈ H ⇒ x ∈ x * H) ⇒ !(x * H = ∅),
	|G| ∈ N ⇒
	(
		∃m: (∈ N, ≤ |G|),
		|G| = |∪({x * H|∀x: ∈ G})|
		= |∪({x[n] * H|∀x[∀n: (∈ N, ≤ m)]: ∈ G})|
		= +[∀n: (∈ N, ≤ m)](|x[n] * H|)
		= |H| × m
	)■
; Proof: end

