; * About S[n], A[n], D[n]
; Theorem: start
	@giacomo.h.abstract_algebra,
	∀n: ∈ N,
	S[n] = gp({c(a, b)|∀(a, b): ∈ N^2, a ≤ n, b ≤ n}),
	A[n] = gp({c(x, y, z)|∀(x, y, z): ∈ N^3, x ≤ n, y ≤ n, z ≤ n}),
	σ: = D[n].σ, τ: = D[n].τ,
	D[n] = gp({σ∘τ, τ}) ⇐ σ∘τ ∘ τ = σ,
	order[D[n]](σ) = n,
	order[D[n]](τ) = 2,
	τ∘σ = σ^-1 ∘ τ
	■
; Theorem: end

