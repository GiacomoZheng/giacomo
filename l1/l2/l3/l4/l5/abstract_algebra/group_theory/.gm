%subgroup%:
(
	∈ relator,

	∀H: ∈ group, ∀G: ∈ group,
	(H %subgroup% G): ⇔ ∀(H ⊆ G, &H.*& = &G.*&, H.e = G.e)
),

tri(∀G: ∈ group): ; the trivial subgroup of G
(
	∈ group,
	D: = {G.e},
	*: = &G.*&,
	e: = G.e
),

/:
(
	∈ operator,

	∀G: ∈ group, ∀H: %subgroup% G,
	*: = &G.*&
	(G / H): = {{x * h|∀h: ∈ H}|∀x: ∈ G} ; the set of all the left cosets of H
)

order[∀G: ∈ group](∀x: ∈ G):
(
	D: = {m|∀m: ∈ N, G.e (G.* x)^m = G.e}
	!(D = ∅) ⇒ = inf(D)
