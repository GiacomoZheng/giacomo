	∀(|orb[μ](s)| ∈ s.N, |μ.G| ∈ s.N) ⇒ |G| = |stab[μ](s)| × |orb[μ](x)|
	■
),

s"Sylow's First Theorem":
(
	@giacomo.h.Group,
	∀G: ∈ group,
	|G| ∈ s.N ⇒
	(
		∀p: ∈ prime_number, ∀n: ∈ s.N, m: = |G|/(p^n),
		∀(m ∈ s.N, gcd(m, p) = 1) ⇒
		(
			∃H: ∈ subgroup(G), |H| = p^n
		)
	)■
),

