; Theorem: start
	@giacomo.h.Group,
	∀G: ∈ group, ∀H: ∈ normal(G), ∀K: ∈ normal(G), *: = &G.*&, e: = G.e,
	∀(H ∩ K = {e}, H * K = G) ⇒ H × K ≌ G■
; Theorem: end
[ proof, ```
	; Lemma: start
		@giacomo.h.Group,
		∀G: ∈ group, ∀H: ∈ normal(G), ∀K: ∈ normal(G), *: = &G.*&, e: = G.e,
		H ∩ K = {e} ⇒ (∀h: ∈ H, ∀k: ∈ K, h * k = k * h)■
	; Lemma: end
	[ proof, ```
		@giacomo.h.Group,
		∀G: ∈ group, ∀H: ∈ normal(G), ∀K: ∈ normal(G), *: = &G.*&,
		H ∩ K = {e} ⇒
		(
			∀h: ∈ H, ∀k: ∈ K,
			(
				∃h': ∈ H, k * h * k^-1 * h^-1 = (k * h * k^-1) * h^-1 = h' * h^-1 ∈ H,
				∃k': ∈ K, k * h * k^-1 * h^-1 = k * (h * k^-1 * h^-1) = k * k' ∈ K
			)
			⇒ k * h * k^-1 * h^-1 ∈ H ∩ K = {e} ⇒ h * k = k * h
		)■
	``` ]
	@giacomo.h.Group,
	∀G: ∈ group, ∀H: ∈ normal(G), ∀K: ∈ normal(G),
	*: = &G.*&, e: = G.e,
	∃φ: ∈ mapping, φ.D = H × K, φ.C = G, f(∀h: ∈ H, ∀k: ∈ K) = h * k,
	(
		φ ∈ homomorphism ⇐
		(
			∀(h1, h2): ∈ H^2, ∀(k1, k2): ∈ K^2,
			φ(h1 * h2, k1 * k2) = h1 * h2 * k1 * k2 = h1 * k1 * h2 * k2
		),
		φ ∈ injection ⇐
		(
			∀(h, k): ∈ H × K, φ(h, k) = e ⇒ h * k = e ⇒ h = k^-1 ⇒ ∀(h ∈ K, k ∈ H) ⇒ ∀(h ∈ {e}, k ∈ {e}) ⇒ (h, k) = (e, e) ⇒ ker(φ) = {(H × K).e}
		),
		φ ∈ surjection ⇐
		(
			φ(H × K) = {h * k|∀(h, k): ∈ H × K} = H * K = G
		)
	)
	⇒ φ ∈ isomorphism ⇒ H × K ≌ G■
``` ]

