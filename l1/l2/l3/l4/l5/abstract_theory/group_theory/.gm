i(∀x: ∈ ◉): = x, ; identity function

inv[∀G: ∈ group](∀g: ∈ G):
(
	[ valid because the unqeness of the inverse in the group  ]
	*: = &G.*&, e: = G.e,
	∃g': ∈ G, g' * g = g * g' = e,
	= g'
),

subgroup(∀G: ∈ group):
(
	⊆ group,

	∀H: ∈ subgroup(G)),
	H ⊆ G,
	H.e = G.e,
	&H.*& = &G.*&
),

order[∀G: ∈ group](∀x: ∈ G):
(
	"it is the cardinal of the smallest subgroup of G with x  in it, generally speaking"
	D: = {m}[∀m: ∈ N, G.e (G.* x)^m = G.e]
	!(D = ∅) ⇒ = inf(D) ; the domian of inf function do not contains ∅. If so, the error will occur
	; otherwise return the cardinal of Z
),

coset[∀G: ∈ group](∀H: ∈ subgroup(G)):
(
	⊆ set,
	
	∀C: ∈ coset[G](H),
	∃g: ∈ G, C = {g G.* h}[∀h: ∈ H] ; left coset indeed
),

≌:
(
	∈ relator,

	∀A: ∈ group, ∀B: ∈ group,
	(A ≌ B) ⇔ (∃f: ∈ isomorphism, ∀(f.D = A, f.C = B)) ; ? undetermined
),

gcd(∀m: ∈ N, ∀n: ∈ N): = sup({m / k, n / k}[∀k: ∈ N] ∩ N),
lcm(∀m: ∈ N, ∀n: ∈ N): = inf({m × k, n × k}[∀k: ∈ N])

prime_number: = {n}[∀n: ∈ N, gcd(n, 1) = n]

; !
gp[∀*: ∈ operator](∀S: ∈ set): = {(* x)^ m (* y) ^ n|∀x: ∈ S, ∀y: ∈ S, ∀m: ∈ N, ∀n: ∈ N}

; 
; this is a group with ∘ as operator, f^-1 as inverse of f

S[∀n: ∈ N]: = sym({m|∀m: ∈ N, 1 ≤ m ≤ n}),

