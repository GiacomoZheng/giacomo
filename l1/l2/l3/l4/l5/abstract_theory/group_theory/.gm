(
	∈ circle,
	Length: = a.cardinal,
	
),

orb[∀μ: ∈ action](∀s: ∈ μ.S): = {μ(g, s)|∀g: ∈ G},
stab[∀μ: ∈ action](∀s: ∈ μ.S): = {g|∀g: ∈ G, μ(g, s) = s},

centralizer[∀G: ∈ group](∀a: ∈ G): = {g|∀g: ∈ G, a G.* g = g G.* a},
center(∀G: ∈ group): = ∩({centralizer[G](a)}[∀a: ∈ G]),

; alternating group ; !
A[∀n: ∈ N]: = {f}[∀f: ∈ S[n], f ∈ even_func],

D[∀n: ∈ N]: ; Dihedral group ; !
= gp[*: ∈ operator]({σ, τ}[∃σ: ∈ ◉, ∃τ: ∈ ◉, order[&*&](σ) = n, order[&*&](τ) = 2, σ * τ = τ (* σ)^2]),

normal(∀G: ∈ group):
(
	⊆ ◉,

	*: = &G.*&

	∀Nor: ∈ normal(G),
	Nor ∈ subgroup(G),
	∀h: ∈ Nor, ∀g: ∈ G, g*h*g^-1 ∈ Nor
),


ker(∀f: ∈ homomorphism): = {x|∀x: ∈ f.D, f(x) = f.C.e},

conjugate(∀G: ∈ group):
(
	⊆ ◉,

	*: = &G.*&

	∀S: ∈ conjugate(G),
	∀h: ∈ S, S = {g * h (* g) ^ -1|∀g: ∈ G} ; !
),

composition_series(∀G: ∈ finite_group): ; !
(
	⊆ set,

	Length: ∈ N,

	∀C: ∈ composition_series(G),
	H[∀n: ∈ {m|∀m: ∈ N, m ≥ 0, m ≤ C.Length}]: ⊆ G,
	H[0] = {G.e}, H[C.Length] = G,
	∀n: ∈ {m|∀m: ∈ N, 0 ≤ m < C.Length},
	H[n] ∈ normal(H[n + 1]),
	H[n + 1] / H[n] ∈ simple_group,
	C = {H[∀n: ∈ {m|∀m: ∈ N, m ≥ 0, m ≤ C.Length}]}
),

