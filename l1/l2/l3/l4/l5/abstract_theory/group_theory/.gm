inv[∀G: ∈ group](∀g: ∈ G):
(
	*: = &G.*&, e: = G.e,
	∃g': ∈ G, g' * g = g * g' = e,
	= g'
),

coset[∀G: ∈ group](∀H: ∈ subgroup(G)):
(
	⊆ set,
	
	∀C: ∈ coset[G](H),
	∃g: ∈ G, C = {g G.* h}[∀h: ∈ H] ; left coset indeed
),

≌:
(
	∈ relator,

	∀A: ∈ group, ∀B: ∈ group,
	(A ≌ B) ⇔ (∃f: ∈ isomorphism, ∀(f.D = A, f.C = B)) ; ? undetermined
),

subgroup(G):
(
	⊆ group,

	∀H: ∈ subgroup(G)),
	H ⊆ G,
	H.e = G.e,
	&H.*& = &G.*&
),

order[∀G: ∈ group](∀x: ∈ G):
(
	D: = {m}[∀m: ∈ N, G.e (G.* x)^m = G.e]
	!(D = ∅) ⇒ = inf(D)
),
; the domian of inf function do not contains ∅. If so, the error will occur

gcd(∀m: ∈ N, ∀n: ∈ N): = sup({m / k, n / k}[∀k: ∈ N] ∩ N),
lcm(∀m: ∈ N, ∀n: ∈ N): = inf({m × k, n × k}[∀k: ∈ N])

prime_number: = {n}[∀n: ∈ N, gcd(n, 1) = n]

gp[∀*: ∈ operator](∀S: ∈ set): ; !
(
	[ if no error happens ]
	∈ group,
	D: = {(* x)^ m (* y) ^ n|∀x: ∈ S, ∀y: ∈ S, ∀m: ∈ N, ∀n: ∈ N}
	*: = &*&,
	e: ...
),

; symmetric group
sym(∀S: ∈ set): = {f|∀f: ∈ permutation, f.D = S},
; this is a group with ∘ as operater

S[∀n: ∈ N]: = sym({m|∀m: ∈ N, m ≤ n})

c(∀a: ∈ sequence): ; !
(
	∈ circle,
	Length: = a.cardinal,
	
),

orb[∀μ: ∈ action](∀s: ∈ μ.S): = {μ(g, s)|∀g: ∈ G},
stab[∀μ: ∈ action](∀s: ∈ μ.S): = {g|∀g: ∈ G, μ(g, s) = s},

centralizer[∀G: ∈ group](∀a: ∈ G): = {g|∀g: ∈ G, a G.* g = g G.* a},
center(∀G: ∈ group): = ∩({centralizer[G](a)}[∀a: ∈ G]),

; alternating group
A[∀n: ∈ N]: = {f}[∀f: ∈ S[n], f ∈ even_func],

D[∀n: ∈ N]: ; Dihedral group
(
	a[∀m: (∈ N, ≤ n)]: = m,
	σ: = c(a),
	b[∀m: (∈ N, ≤ n / 2)]: = c(m, 2 + n - m)
	B[0]: i,
	B[∀m: (∈ N, ≤ n / 2)]: = B[m - 1] ∘ b[m],
	τ: = B[sup({∀m: (∈ N, ≤ n / 2)})],
	= gp({σ, τ})
),

normal(∀G: ∈ group):
(
	⊆ ◉,

	*: = &G.*&

	∀Nor: ∈ normal(G),
	Nor ∈ subgroup(G),
	∀h: ∈ Nor, ∀g: ∈ G, g*h*g^-1 ∈ Nor
),


ker(∀f: ∈ homomorphism): = {x|∀x: ∈ f.D, f(x) = f.C.e},

conjugate(∀G: ∈ group):
(
	⊆ ◉,

	*: = &G.*&

	∀S: ∈ conjugate(G),
	∀h: ∈ S, S = {g * h (* g) ^ -1|∀g: ∈ G} ; !
),

composition_series(∀G: ∈ finite_group): ; !
(
	⊆ set,

	Length: ∈ N,

	∀C: ∈ composition_series(G),
	H[∀n: ∈ {m|∀m: ∈ N, m ≥ 0, m ≤ C.Length}]: ⊆ G,
	H[0] = {G.e}, H[C.Length] = G,
	∀n: ∈ {m|∀m: ∈ N, 0 ≤ m < C.Length},
	H[n] ∈ normal(H[n + 1]),
	H[n + 1] / H[n] ∈ simple_group,
	C = {H[∀n: ∈ {m|∀m: ∈ N, m ≥ 0, m ≤ C.Length}]}
),

