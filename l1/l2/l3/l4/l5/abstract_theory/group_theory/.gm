fac:
(
	∈ function,
	fac(0) = 1,
	fac(∀n: (∈ s.Z, n > 0)) = fac(n - 1) * n
),

inv[∀G: ∈ group](∀g: ∈ G):
(
	[ valid because the unqeness of the inverse in the group  ]
	*: = &G.*&, e: = G.e,
	∃g': ∈ G, g' * g = g * g' = e,
	= g'
),

order[∀G: ∈ group](∀x: ∈ G):
(
	"it is the cardinal of the smallest subgroup of G with x  in it, generally speaking"
	D: = {m}[∀m: ∈ N, G.e (G.* x)^m = G.e]
	!(D = ∅) ⇒ = inf(D) ; the domian of inf function do not contains ∅. If so, the error will occur
	; + otherwise return the cardinal of Z
),

ker(∀f: ∈ group.homomorphism): = {g|f(g) = f.C.e}[∀g: ∈ f.D], 

$:
(
	"to get a permutation corresponding to the element in the group",
	∈ symbol,
	∀σ: ∈ action, ∀x: ∈ σ.G,
	∃f(∀a: ∈ σ.S): = σ(x, a),
	σ$x = f
),

subgroup(∀G: ∈ group): ; different from other books
(
	⊆ group,

	∀H: ∈ subgroup(G),
	H ⊆ G,
	H.e = G.e,
	&H.*& = &G.*&
),

C[∀G: ∈ group](∀A: ⊆ G): = {g|∀a: ∈ A, a G.* g = g G.* a}[∀g: ∈ G], ; centralizer

Z(∀G: ∈ group): = C[G](G), ; center

N[∀G: ∈ group](∀A: ⊆ G): = {g|∀a: ∈ A, g G.* a G.* inv[G](g) ∈ A}[∀g: ∈ G], ; normalizer

stabilizer[∀μ: ∈ action](∀s: ∈ μ.S): = {g|μ(g, s) = s}[∀g: ∈ G],

orb[∀μ: ∈ action](∀s: ∈ μ.S): = {μ(g, s)}[∀g: ∈ G],

coset[∀G: ∈ group](∀H: ∈ subgroup(G)):
(
	⊆ set,
	
	∀C: ∈ coset[G](H),
	∃g: ∈ G, C = {g G.* h}[∀h: ∈ H] ; left coset indeed
),

≌:
